{
  "manifest_version": 2,
  "name": "Kemono_Downloader",
  "description": "kemono.su 게시글에서 우클릭하여 저장",
  "version": "1.0.3",
  "author": "AwerQxcv",
  "icons": {
    "48": "icon_48.png",
    "96": "icon_96.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://kemono.su/*"],
      "js": ["Kemono_Downloader.js"]
    }
  ],
  "background": {
    "scripts": ["event_page.js"]
  },
  "permissions": [
    "downloads",
    "storage",
    "tabs",
    "contextMenus",
    "*://kemono.su/*"
  ],
  "web_accessible_resources": ["icon.svg"],
  "options_ui": {
    "page": "settings.html"
  },
  "homepage_url": "https://google.com/"
}
